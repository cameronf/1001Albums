<div id="friends_stats_scroll" class="scrolling_albums">
	<% for stat in @all_stats %>
    <div class="span3">
		  <%= render(:partial => "stats_block", :locals => {:leader => stat,:fb_info => @fb_all.select {|x| x["id"] == stat.fb_user_id}.first }) %>
	  </div>
  <% end %>
</div>
<script>
  do_js_set_div_height('friends_stats_scroll');
</script>
